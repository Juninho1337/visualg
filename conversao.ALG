Algoritmo "conversao_ctof"
// Seção de declarações

Var celsius, fahrenheit:real

Inicio
     escreva("Digite a temperatura em °C: ")
     leia (celsius)

     fahrenheit <- (celsius * 1.8) + 32
          Escreva (celsius, " graus Celsius ", "é igual a ", fahrenheit, " graus Fahrenheit")

Fimalgoritmo